 		},
 "eventHandler":{
 	"mysql_statistic":{
 		"enable":true,
 		"pakage":"mysql",
 		"modul":"mapping",
 		"class":"server",
 		"config":{
 			"host":"192.168.3.100",
 			"user":"hmceasy",
 			"password":"test1",
 			"db":"hmc",
 			"table":"statistic",
 			"mapping":
 			  [
 				{"value":"value"},
 				{"device":"deviceID"},
 				{"timestamp":"lastchange"}
 			  ] 				
 			}
 	},
    "hm_XML-API":{
 		"enable":false,
 		"pakage":"homematic",
 		"modul":"xmlapi",
 		"class":"server",
 		"config":{
 			"host":"192.168.3.100",
 			"mapping":
 			  [
 				{"value.ks300_rain@cul.rasp-test":"765"},
 				{"value.ks300_temperatur@cul.rasp-test":"876"},
 				{"value.28-002999287@onewire.rasp-test":"879"}
 			  ] 				
 			}
 	}
 },
 "gateways":{
 	"HMC_Connector":{
 		"enable":false,
 		"pakage":"hmc_connector",
 		"config":{
 			"name":"hmc_connector"
 			}
 		},
 	"cul_868":{
 		"enable":false,
 		"pakage":"cul",
 		"modul":"cul",
 		"class":"server",
 		"config":{
 			"usbport":"/dev/ttyAMA0",
            "baudrate":"9600",
            "timeout":true,
 			"gateway":"cul"
 			}
 		},
 	"onewire":{
 		"enable":true,
 		"pakage":"raspberry",
 		"modul":"ds1820",
 		"class":"sensor",
 		"config":{
 			"intervall":60,
			"typ":"ds1820",
			"tempDiv":0.5,
			"path":"/sys/bus/w1/devices/",
			"gateway":"onewire",
			"devices":{}
            }
 		},
 	"hmc_rpc_rf":{
 		"enable":false,
 		"pakage":"homematic",
 		"modul":"rpc_server",
 		"class":"server",
 		"config":{
 			"name":"hmc_rpc_rf",
			"hm_ip":"192.168.3.90",
			"hm_port":"2000",
			"hm_interface_id":"122",
			"rpc_port":"5050",
			"rpc_ip":"192.168.3.87"
 			}
 		},
 	"hmc_rpc_wired":{
 		"enable":false,
 		"pakage":"homematic",
 		"modul":"rpc_server",
 		"class":"server",
 		"config":{
 			"name":"hmc_rpc_wired",
			"hm_ip":"192.168.3.90",
			"hm_port":"2000",
			"hm_interface_id":"123",
			"rpc_port":"5051"
 			}
 		}
 },
